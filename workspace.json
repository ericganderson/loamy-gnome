{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "inlineStyle": true,
      "inlineTemplate": true,
      "prefix": "nrg",
      "skipTests": true,
      "tags": "type:app",
      "viewEncapsulation": "None"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "prefix": "nrg",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush",
      "displayBlock": true
    }
  },
  "defaultProject": "energy-insights-app",
  "projects": {
    "energy-insights-app": {
      "projectType": "application",
      "root": "apps/energy-insights-app",
      "sourceRoot": "apps/energy-insights-app/src",
      "prefix": "nrg",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/energy-insights-app",
            "index": "apps/energy-insights-app/src/index.html",
            "main": "apps/energy-insights-app/src/main.ts",
            "polyfills": "apps/energy-insights-app/src/polyfills.ts",
            "tsConfig": "apps/energy-insights-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "libs/root/assets/src",
                "output": "./"
              },
              {
                "glob": "**/*",
                "input": "libs/root/assets/src/assets",
                "output": "assets"
              }
            ],
            "styles": ["libs/root/styles/src/index.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/energy-insights-app/src/environments/environment.ts",
                  "with": "apps/energy-insights-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "energy-insights-app:build:production"
            },
            "development": {
              "browserTarget": "energy-insights-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "energy-insights-app:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/energy-insights-app/src/**/*.ts",
              "apps/energy-insights-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/energy-insights-app"],
          "options": {
            "jestConfig": "apps/energy-insights-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "root-assets": {
      "projectType": "library",
      "root": "libs/root/assets",
      "sourceRoot": "libs/root/assets/src",
      "targets": {}
    },
    "root-styles": {
      "projectType": "library",
      "root": "libs/root/styles",
      "sourceRoot": "libs/root/styles/src",
      "targets": {}
    }
  }
}
